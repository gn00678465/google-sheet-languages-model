  "type": "module",
  "license": "MIT",
  "keywords": [
    "i18n",
    "google-sheet",
    "google-sheet-i18n",
    "javascript",
    "typescript"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
