    "i18n:push": "tsx example/push.ts",
    "i18n:pull:nest": "tsx example/pull.ts --type=nest",
    "i18n:pull:flat": "tsx example/pull.ts --type=flat"
  },
  "dependencies": {
    "googleapis": "^150.0.1",
